<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.mapper.IndexMapper">
    <select id="showInfo" resultType="com.lx.pojo.Index">
        select * from designone where id = 1
    </select>

    <select id="showSpeech" resultType="com.lx.pojo.Index">
        select deanImg, speechOne, speechTwo, speechThree from designone
    </select>

    <select id="showOrgImg" resultType="com.lx.pojo.Index">
        select orgUrl from designone
    </select>

    <select id="showPartner" resultType="com.lx.pojo.Partner">
        select * from partner
    </select>

    <select id="showAll" resultType="com.lx.pojo.FirstPage">
        select * from firstpage
    </select>
</mapper>