<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.mapper.HoronMapper">

    <select id="findAll" resultType="com.lx.pojo.Horon" parameterType="Horon">
        select * from horon_info
    </select>
    <select id="findById" resultType="com.lx.pojo.Horon">
        select * from horon_info where id = #{id}
    </select>
</mapper>